<?xml version="1.0" encoding="UTF-8"?>
<thing:thing-descriptions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                          xmlns:thing="http://eclipse.org/smarthome/schemas/thing-description/v1.0.0"
                          bindingId="lwm2mleshan"
                          xsi:schemaLocation="http://eclipse.org/smarthome/schemas/thing-description/v1.0.0 http://eclipse.org/smarthome/schemas/thing-description-1.0.0.xsd">
   <thing-type id="10246">
      <supported-bridge-type-refs>
         <bridge-type-ref id="client"/>
      </supported-bridge-type-refs>
      <label>CrowdBox Measurements</label>
      <description><![CDATA[This LWM2M Object provides CrowdBox-related measurements such as serving cell parameters, backhaul timing advance, and neighbour cell reports.]]></description>
      <!--Optional channel "Cumulative Backhaul Timing Advance" (4) will be added dynamically-->
      <channel-groups>
         <channel-group id="0" typeId="0"><!--Channel "Serving Cell ID" (0)--></channel-group>
         <channel-group id="1" typeId="1"><!--Channel "Serving Cell RSRP" (1)--></channel-group>
         <channel-group id="2" typeId="2"><!--Channel "Serving Cell RSRQ" (2)--></channel-group>
         <channel-group id="3" typeId="3"><!--Channel "Serving Cell SINR" (3)--></channel-group>
         <channel-group id="5" typeId="5"><!--Channel "Neighbour Cell Report" (5)--></channel-group>
      </channel-groups>
      <properties>
         <property name="ObjectID">10246</property>
         <property name="ObjectURN">urn:oma:lwm2m:x:10246</property>
         <property name="MultipleInstances">Single</property>
         <property name="Mandatory">Mandatory</property>
      </properties>
   </thing-type>
   <channel-group-type id="0"><!--MultiInstances: Single-->
      <label>Serving Cell ID</label>
      <channels>
         <channel id="0" typeId="0"><!--Channel "Serving Cell ID", ID: 0, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="0">
      <item-type>Number</item-type>
      <label>Serving Cell ID</label>
      <description><![CDATA[Serving cell ID as specified by the cellIdentity field broadcast in SIB1 of the serving cell (see TS 36.331).]]></description>
      <tags>
         <tag>Integer</tag>
      </tags>
      <state readOnly="true"/>
   </channel-type>
   <channel-group-type id="1"><!--MultiInstances: Single-->
      <label>Serving Cell RSRP</label>
      <channels>
         <channel id="0" typeId="1"><!--Channel "Serving Cell RSRP", ID: 1, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="1">
      <item-type>Number</item-type>
      <label>Serving Cell RSRP</label>
      <description><![CDATA[Serving cell RSRP, as defined in TS 36.133, Section 9.1.4. Range: RSRP_00; RSRP_01 .. RSRP_97]]></description>
      <tags>
         <tag>Integer</tag>
      </tags>
      <state readOnly="true"/>
   </channel-type>
   <channel-group-type id="2"><!--MultiInstances: Single-->
      <label>Serving Cell RSRQ</label>
      <channels>
         <channel id="0" typeId="2"><!--Channel "Serving Cell RSRQ", ID: 2, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="2">
      <item-type>Number</item-type>
      <label>Serving Cell RSRQ</label>
      <description><![CDATA[Serving cell RSRQ, as defined in TS 36.133, Section 9.1.7. Range: RSRQ_-30; RSRQ_-29 .. RSRQ_46]]></description>
      <tags>
         <tag>Integer</tag>
      </tags>
      <state readOnly="true"/>
   </channel-type>
   <channel-group-type id="3"><!--MultiInstances: Single-->
      <label>Serving Cell SINR</label>
      <channels>
         <channel id="0" typeId="3"><!--Channel "Serving Cell SINR", ID: 3, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="3">
      <item-type>Number</item-type>
      <label>Serving Cell SINR</label>
      <description><![CDATA[SINR of serving cell as estimated by the CrowdBox. Note that this is a proprietary measurement dependent on the UE chipset manufacturer. The UE chipset used should be stated in the accompanying product documentation.]]></description>
      <tags>
         <tag>Integer</tag>
      </tags>
      <state readOnly="true"/>
   </channel-type>
   <channel-group-type id="4"><!--MultiInstances: Single-->
      <label>Cumulative Backhaul Timing Advance</label>
      <channels>
         <channel id="0" typeId="4"><!--Channel "Cumulative Backhaul Timing Advance", ID: 4, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="4" advanced="true">
      <item-type>Number</item-type>
      <label>Cumulative Backhaul Timing Advance</label>
      <description><![CDATA[The cumulative timing advance signalled by the current serving cell to the CrowdBox. This is the sum of the initial timing advance signalled in the MAC payload of the Random Access Response (11 bits, 0 .. 1282) and subsequent adjustments signalled in the MAC PDU of DL-SCH transmissions (6 bits, -31 .. 32). See TS 36.321 for details.]]></description>
      <tags>
         <tag>Integer</tag>
      </tags>
      <state readOnly="true"/>
   </channel-type>
   <channel-group-type id="5"><!--MultiInstances: Multiple-->
      <label>Neighbour Cell Report</label>
      <channels>
         <channel id="0" typeId="5"><!--Channel "Neighbour Cell Report", ID: 5, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="5">
      <item-type>String</item-type>
      <label>Neighbour Cell Report</label>
      <description><![CDATA[A link to the "Neighbour Cell Report" object for each neighbour cell of the CrowdBox.]]></description>
      <state readOnly="true"/>
   </channel-type>
</thing:thing-descriptions>
