   <thing-type id="3344">
      <supported-bridge-type-refs>
         <bridge-type-ref id="lwm2mBridgeThing"/>
      </supported-bridge-type-refs>
      <label>Up/Down Control</label>
      <description><![CDATA[This IPSO object is used to report the state of an up/down control element like a pair of push buttons or a rotary encoder. Counters for increase and decrease operations are provided for counting pulses from a quadrature encoder.]]></description>
      <!--Optional channel "Up Counter" (5541) will be added dynamically-->
      <!--Optional channel "Down Counter" (5542) will be added dynamically-->
      <!--Optional channel "Application Type" (5750) will be added dynamically-->
