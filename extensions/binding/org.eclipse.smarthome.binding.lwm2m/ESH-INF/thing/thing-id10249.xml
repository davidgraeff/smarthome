<?xml version="1.0" encoding="UTF-8"?>
<thing:thing-descriptions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                          xmlns:thing="http://eclipse.org/smarthome/schemas/thing-description/v1.0.0"
                          bindingId="lwm2mleshan"
                          xsi:schemaLocation="http://eclipse.org/smarthome/schemas/thing-description/v1.0.0 http://eclipse.org/smarthome/schemas/thing-description-1.0.0.xsd">
   <thing-type id="10249">
      <supported-bridge-type-refs>
         <bridge-type-ref id="client"/>
      </supported-bridge-type-refs>
      <label>Connected UE Report</label>
      <description><![CDATA[This LWM2M Object provides a range of information related to the connected UEs.]]></description>
      <!--Optional channel "Cumulative Timing Advance per Connected User" (4) will be added dynamically-->
      <channel-groups>
         <channel-group id="0" typeId="0"><!--Channel "Connected User MMEC" (0)--></channel-group>
         <channel-group id="1" typeId="1"><!--Channel "Connected User M-TMSI" (1)--></channel-group>
         <channel-group id="2" typeId="2"><!--Channel "Serving Cell (CrowdBox) eNB RSRP" (2)--></channel-group>
         <channel-group id="3" typeId="3"><!--Channel "Serving Cell (CrowdBox) eNB RSRQ" (3)--></channel-group>
         <channel-group id="5" typeId="5"><!--Channel "Last downlink CQI report per Connected User" (5)--></channel-group>
         <channel-group id="6" typeId="6"><!--Channel "Cumulative Downlink Throughput per Connected User" (6)--></channel-group>
         <channel-group id="7" typeId="7"><!--Channel "Cumulative Uplink Throughput per Connected User" (7)--></channel-group>
         <channel-group id="8" typeId="8"><!--Channel "Neighbour Cell Report" (8)--></channel-group>
      </channel-groups>
      <properties>
         <property name="ObjectID">10249</property>
         <property name="ObjectURN">urn:oma:lwm2m:x:10249</property>
         <property name="MultipleInstances">Multiple</property>
         <property name="Mandatory">Mandatory</property>
      </properties>
   </thing-type>
   <channel-group-type id="0"><!--MultiInstances: Single-->
      <label>Connected User MMEC</label>
      <channels>
         <channel id="0" typeId="0"><!--Channel "Connected User MMEC", ID: 0, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="0">
      <item-type>Number</item-type>
      <label>Connected User MMEC</label>
      <description><![CDATA[MMEC signalled by the UE to the eNB in the RRCConnectionRequest message (see TS 36.331).]]></description>
      <tags>
         <tag>Integer</tag>
      </tags>
      <state readOnly="true"/>
   </channel-type>
   <channel-group-type id="1"><!--MultiInstances: Single-->
      <label>Connected User M-TMSI</label>
      <channels>
         <channel id="0" typeId="1"><!--Channel "Connected User M-TMSI", ID: 1, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="1">
      <item-type>Number</item-type>
      <label>Connected User M-TMSI</label>
      <description><![CDATA[M-TMSI signalled by the UE to the eNB in the RRCConnectionRequest message (see TS 36.331).]]></description>
      <tags>
         <tag>Integer</tag>
      </tags>
      <state readOnly="true"/>
   </channel-type>
   <channel-group-type id="2"><!--MultiInstances: Single-->
      <label>Serving Cell (CrowdBox) eNB RSRP</label>
      <channels>
         <channel id="0" typeId="2"><!--Channel "Serving Cell (CrowdBox) eNB RSRP", ID: 2, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="2">
      <item-type>Number</item-type>
      <label>Serving Cell (CrowdBox) eNB RSRP</label>
      <description><![CDATA[The RSRP of the CrowdBox eNB, as defined in TS 36.133, Section 9.1.4. Range: RSRP_00; RSRP_01 .. RSRP_97]]></description>
      <tags>
         <tag>Integer</tag>
      </tags>
      <state readOnly="true"/>
   </channel-type>
   <channel-group-type id="3"><!--MultiInstances: Single-->
      <label>Serving Cell (CrowdBox) eNB RSRQ</label>
      <channels>
         <channel id="0" typeId="3"><!--Channel "Serving Cell (CrowdBox) eNB RSRQ", ID: 3, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="3">
      <item-type>Number</item-type>
      <label>Serving Cell (CrowdBox) eNB RSRQ</label>
      <description><![CDATA[The RSRQ of the CrowdBox eNB, as defined in TS 36.133, Section 9.1.7. Range: RSRQ_-30; RSRQ_-29 .. RSRQ_46]]></description>
      <tags>
         <tag>Integer</tag>
      </tags>
      <state readOnly="true"/>
   </channel-type>
   <channel-group-type id="4"><!--MultiInstances: Single-->
      <label>Cumulative Timing Advance per Connected User</label>
      <channels>
         <channel id="0" typeId="4"><!--Channel "Cumulative Timing Advance per Connected User", ID: 4, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="4" advanced="true">
      <item-type>Number</item-type>
      <label>Cumulative Timing Advance per Connected User</label>
      <description><![CDATA[The cumulative timing advance signalled by the eNB to each currently connected UE. This is the sum of the initial timing advance signalled in the MAC payload of the Random Access Response (11 bits, 0 .. 1282) and subsequent adjustments signalled in the MAC PDU of DL-SCH transmissions (6 bits, -31 .. 32). See TS 36.321 for details.]]></description>
      <tags>
         <tag>Integer</tag>
      </tags>
      <state readOnly="true"/>
   </channel-type>
   <channel-group-type id="5"><!--MultiInstances: Single-->
      <label>Last downlink CQI report per Connected User</label>
      <channels>
         <channel id="0" typeId="5"><!--Channel "Last downlink CQI report per Connected User", ID: 5, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="5">
      <item-type>Number</item-type>
      <label>Last downlink CQI report per Connected User</label>
      <description><![CDATA[The last downlink wideband CQI reported by a connected user the eNB. The CQI format is defined in Table 7.2.3-1 of TS 36.213.]]></description>
      <tags>
         <tag>Integer</tag>
      </tags>
      <state readOnly="true"/>
   </channel-type>
   <channel-group-type id="6"><!--MultiInstances: Single-->
      <label>Cumulative Downlink Throughput per Connected User</label>
      <channels>
         <channel id="0" typeId="6"><!--Channel "Cumulative Downlink Throughput per Connected User", ID: 6, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="6">
      <item-type>Number</item-type>
      <label>Cumulative Downlink Throughput per Connected User</label>
      <description><![CDATA[The total number of MAC bytes sent to the connected user over the immediately preceding period specified by the "Cumulative Measurement Window" field.]]></description>
      <tags>
         <tag>Integer</tag>
      </tags>
      <state readOnly="true"/>
   </channel-type>
   <channel-group-type id="7"><!--MultiInstances: Single-->
      <label>Cumulative Uplink Throughput per Connected User</label>
      <channels>
         <channel id="0" typeId="7"><!--Channel "Cumulative Uplink Throughput per Connected User", ID: 7, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="7">
      <item-type>Number</item-type>
      <label>Cumulative Uplink Throughput per Connected User</label>
      <description><![CDATA[The total number of MAC bytes received from the connected user over the immediately preceding period specified by the "Cumulative Measurement Window" field.]]></description>
      <tags>
         <tag>Integer</tag>
      </tags>
      <state readOnly="true"/>
   </channel-type>
   <channel-group-type id="8"><!--MultiInstances: Multiple-->
      <label>Neighbour Cell Report</label>
      <channels>
         <channel id="0" typeId="8"><!--Channel "Neighbour Cell Report", ID: 8, InstanceID: 0--></channel>
      </channels>
   </channel-group-type>
   <channel-type id="8">
      <item-type>String</item-type>
      <label>Neighbour Cell Report</label>
      <description><![CDATA[A link to the "Neighbour Cell Report" object for each neighbour cell reported to the CrowdBox by the connected UE]]></description>
      <state readOnly="true"/>
   </channel-type>
</thing:thing-descriptions>
