         <channel-group id="5532" typeId="5532"><!--Channel "Increase input State" (5532)--></channel-group>
         <channel-group id="5533" typeId="5533"><!--Channel "Decrease Input State" (5533)--></channel-group>
      </channel-groups>
      <properties>
         <property name="ObjectID">3344</property>
         <property name="ObjectURN">urn:oma:lwm2m:ext:3344</property>
         <property name="MultipleInstances">Multiple</property>
         <property name="Mandatory">Optional</property>
      </properties>
   </thing-type>
